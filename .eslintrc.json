{
    "extends": [
        "@dvukovic/eslint-config/core",
        "@dvukovic/eslint-config/typescript",
        "@dvukovic/eslint-config/react",
        "@dvukovic/eslint-config/react-native"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "ignorePatterns": [
        "babel.config.js",
        "jest.config.js",
        "metro.config.js"
    ],
    "settings": {
        "import/ignore": ["node_modules/react-native/index\\.js$"]
    }
}
